{
    "name": "Taskwarror for Chrome",
    "version": "1.0",
    "description": "List, add and modify tasks from Chrome",
    "permissions": [
      "storage",
      "alarms",
      "https://inthe.am/api/v2/*"
    ],
		"background": {
		  "scripts": [
        "sync.js",
        "background.js"
      ],
      "persistent":false
		},
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      }
    },
    "icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
    "options_page": "options.html",
    "manifest_version": 2
}
